<h1 class="font-bold text-4xl text-blue-500">Welcome#index</h1>

<p>Find me in app/views/welcome/index.html.erb</p>

<%= turbo_frame_tag "contact-us-form" do %>
  <%= link_to 'Contact Us', contact_us_new_path %>
<% end %>

<%= turbo_stream_from "messages123" %>
<div id="messages">
  <% @messages.each do |message| %>
    <%= render partial: 'contact_us/message', locals: { message: message } %>
  <% end %>
</div>
